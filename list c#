using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<string> listeChaines = new List<string>();
        listeChaines.Add("1");
        listeChaines.Add("2");
        listeChaines.Add("3");

        listeChaines.RemoveAt(0);

        listeChaines.Remove("3");

        listeChaines[1] = "2";

        foreach (string chaine in listeChaines)
        {
            Console.WriteLine(chaine);
        }
    }
}
